{
    "name": "webignition/css-validator-wrapper",
    "type": "library",
    "description": "PHP wrapper for the W3C css-validator.jar command line tool",
    "keywords": ["css", "validator"],
    "homepage": "https://github.com/webignition/css-validator-wrapper",
    "license": "MIT",

    "require": {
        "php": ">=5.3.2",
        "webignition/css-validator-output-parser": ">=2.1.2",
        "webignition/web-resource-service":">=0.2",
        "webignition/absolute-url-deriver":">=1.4.1"
    },

    "autoload": {
        "psr-0": {
            "webignition\\Tests": "tests/",
            "": "src/"
        }
    },

    "require-dev": {
        "symfony/class-loader": "*",
        "phpunit/phpunit": "3.7.*"
    }
}
